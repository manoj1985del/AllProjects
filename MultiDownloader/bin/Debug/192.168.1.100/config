#			Master config file
#The file contains the default message selected by the user 
#and list of proper(message file and corresponding config file exist in the printer)
#and valid message files

##############messages##############################
#message_default is the name of the message file currently being used in printing.
message_default=456.dle
#message_options contains the list of messages present on the printer
message_options=123.dle,456.dle,TEST142.dle

##############Languages##############################
language_default=English
#language_options option is used in tcp/ip this contains the values of files present in folders
language_options=English
##############date Variables##############################
#date_default has the current date of the printer in the config file(config file is requestd by host PC).
#The varaible is used for setting the controller(printer) date from TCP/IP
#The format of the date_default value depends on the 'date_format_default' variable.
#E.g if 'date_format_default' is  %d/%m/%Y then date_default=11/11/2011
#If the user wants to setdate the date should be in the format of date_format_default.
#date_format_default can be any combination %d(day) %m(month) %Y(Year)
#This is the format used in GUI of the printer
#delimiters which can be used between '/' '-'
date_format_default=%d/%m/%Y
date_default=13/02/2017

##############time Variables##############################
#time_default has the current time of the printer in the config file(config file is requestd by host PC).
#The varaible is used for setting the controller(printer) time from TCP/IP
#The format of the time_default value depends on the "time_format_default" variable.
#E.g if "time_format_default" is "%H:%I:%S" then date_default=12:30:12
#If the user wants to set time the time should be in the format of time_format_default.
time_format_default=%H:%M:%S
time_default=22:23:24
#hour format can be 12/24 if hour format is 12 time indicator AM/PM is used
hour_format_default=24
dist_betwn_heads=525
h1h2_pixel_overlap=0
#network settings
ip_default=192.168.1.100
subnet_mask_default=255.255.255.0
gateway_default=192.168.1.1
dns_default=192.168.1.1
port_default=10100
purge_repeat_counter=30
###### Cartridge Ink status related Variables########### 
# ink_status_h1/h2 gives the percentage of Ink used in the cartridge 
cartridge1_pixel_capacity=30303
million_dots_h1=2
ink_status_h1=0
cartridge2_pixel_capacity=1000
million_dots_h2=0
ink_status_h2=0
dot_cnt_h1=0
dot_cnt_h2=0
#Following are internal variables(ignore_head_health,encoder_signal_usage).
#If manually changed reboot the system. 
ignore_head_health=0
encoder_signal_usage=0
#System info values
serial_number=1078
validity=1
start_date=-
end_date=-
f1_info=Arial_290
f2_info=Arial_100
fwr_ver=14.2  
printer_line=0
printer_cood=0

